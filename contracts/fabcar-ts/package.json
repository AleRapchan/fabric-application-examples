{
    "name": "typescript",
    "version": "0.0.3",
    "description": "",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "lint": "tslint -c tslint.json 'src/**/*.ts'",
        "pretest": "npm run lint",
        "test": "nyc mocha -r ts-node/register src/**/*.spec.ts",
        "start": "fabric-chaincode-node start",
        "build": "tsc",
        "build:watch": "tsc -w",
        "prepublishOnly": "npm run build"
    },
    "engineStrict": true,
    "author": "",
    "license": "",
    "dependencies": {
        "fabric-contract-api": "file:../../../go/src/github.com/hyperledger/fabric-chaincode-node/fabric-contract-api/fabric-contract-api-1.3.0-snapshot.tgz",
        "fabric-shim": "file:../../../go/src/github.com/hyperledger/fabric-chaincode-node/fabric-shim/fabric-shim-1.3.0-snapshot.tgz"
    },
    "devDependencies": {
        "@types/chai": "^4.1.6",
        "@types/mocha": "^5.2.3",
        "@types/node": "^10.11.5",
        "@types/sinon": "^5.0.5",
        "@types/sinon-chai": "^3.2.0",
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "nyc": "^12.0.2",
        "sinon": "^6.3.5",
        "sinon-chai": "^3.2.0",
        "ts-node": "^7.0.0",
        "tslint": "^5.10.0",
        "typescript": "^2.9.2"
    },
    "nyc": {
        "extension": [
            ".ts",
            ".tsx"
        ],
        "exclude": [
            "coverage/**",
            "dist/**"
        ],
        "reporter": [
            "text-summary",
            "html"
        ],
        "all": true,
        "check-coverage": true,
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
    }
}
